{
    "version": "2.0.0",
    "manifest_version": 2,
    "name": "cursors",
    "description": " 2 Day TechFest conducted by CSE Dept at ANITS",
    "icons": {
        "128": "./images/icon.png",
        "48": "./images/icon.png",
        "16": "./images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "./images/icon.png",
            "48": "./images/icon.png",
            "16": "./images/icon.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "./new-tab.html"
    },
    "background": {
        "scripts": [
            "./background.js"
        ],
        "persistent": false
    },
    "web_accessible_resources": [
        "images/icon.png"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'; img-src chrome://favicon;",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "topSites",
        "storage",
        "tabs",
        "chrome://favicon/*"
    ]
}