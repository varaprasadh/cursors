{
    "version":"1.0.1",
    "manifest_version":2,
    "name": "cursors",
    "description": " 2 Day TechFest conducted by CSE Dept at ANITS",
    "icons":{
        "128":"./images/icon.png",
        "48":"./images/icon.png",
        "16":"./images/icon.png"
    },
    "browser_action":{
        "default_icon":{
            "128":"./images/icon.png",
            "48":"./images/icon.png",
            "16":"./images/icon.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "./newTab.html"
      },
    "background": {
        "scripts": ["./js/background.js"],
        "persistent": false
    },
"web_accessible_resources": ["images/icon.png"],
    "permissions": ["http://*/*", "https://*/*", "topSites", "storage", "tabs"]
}