{
    "version":"1.0",
    "manifest_version":2,
    "name": "cursors",
    "description": " 2 Day TechFest conducted by CSE Dept at ANITS",
    "icons":{
        "128":"./images/icon.png",
        "48":"./images/icon.png",
        "16":"./images/icon.png"
    },
    "browser_action":{
        "default_icon":{
            "128":"./images/icon.png",
            "48":"./images/icon.png",
            "16":"./images/icon.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "./newTab.html"
      },
    "background": {
        "scripts": ["./js/background.js"],
        "persistent": false
    },
    "permissions": ["topSites","storage","tabs"]
}